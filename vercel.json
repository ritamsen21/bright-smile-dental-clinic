{
  "buildCommand": "cd frontend && npm ci || npm install && npm run build:prod",
  "outputDirectory": "frontend/dist/frontend/browser",
  "installCommand": "cd frontend && npm install --legacy-peer-deps",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
